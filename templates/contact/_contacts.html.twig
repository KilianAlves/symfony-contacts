<ul class="contacts">
    {% for contact in contacts %}
        <li class="contact">
            <a href="{{ path('app_contact_show', {'id':contact.id} ) }}">
                <span class="lastname">{{ contact.lastname }}</span>,<span class="firstname"> {{ contact.firstname }} </span>,<span class="categorie">{{ contact.category.name ?? 'Rien' }}</span>
            </a>
        </li>
    {% endfor %}
</ul>